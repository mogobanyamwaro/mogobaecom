}>\n      <div className=\"card p-3 rounded\">\n        <img className=\"card-img-top mx-auto\" src={product.images[0].url} />\n        <div className=\"card-body d-flex flex-column\">\n          <h5 className=\"card-title\">\n            <Link to={`/product/${product._id}`}>{product.name}</Link>\n          </h5>\n          <div className=\"ratings mt-auto\">\n            <div className=\"rating-outer\">\n              <div\n                className=\"rating-inner\"\n                style={{ width: `${(product.ratings / 5) * 100}%` }}\n              ></div>\n            </div>\n            <span id=\"no_of_reviews\">({product.numOfReviews} Reviews)</span>\n          </div>\n          <p className=\"card-text\">${product.price}</p>\n          <Link\n            to={`/product/${product._id}`}\n            id=\"view_btn\"\n            className=\"btn btn-block\"\n          >\n            View Details\n          </Link>\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default Product;\n","/home/douglas/Pictures/deploy ecommerce to aws/frontend/src/actions/productActions.js",[],"/home/douglas/Pictures/deploy ecommerce to aws/frontend/src/responsive.js",[],"/home/douglas/Pictures/deploy ecommerce to aws/frontend/src/components/layout/Search.js",[],"/home/douglas/Pictures/deploy ecommerce to aws/frontend/src/actions/cartActions.js",[],"/home/douglas/Pictures/deploy ecommerce to aws/frontend/src/config.js",[],"/home/douglas/Pictures/deploy ecommerce to aws/frontend/src/actions/orderActions.js",[],"/home/douglas/Pictures/deploy ecommerce to aws/frontend/src/components/review/ListReviews.js",[],"/home/douglas/Pictures/deploy ecommerce to aws/frontend/src/constants/productConstants.js",[],"/home/douglas/Pictures/deploy ecommerce to aws/frontend/src/constants/userConstants.js",[],"/home/douglas/Pictures/deploy ecommerce to aws/frontend/src/constants/cartConstants.js",[],"/home/douglas/Pictures/deploy ecommerce to aws/frontend/src/constants/orderConstants.js",[],"/home/douglas/Pictures/deploy ecommerce to aws/frontend/src/components/cart/CheckoutSteps.js",[],"/home/douglas/Pictures/deploy ecommerce to aws/frontend/src/components/layout/Slider.jsx",["265"],"import { ArrowLeftOutlined, ArrowRightOutlined } from '@material-ui/icons';\nimport { useState, useEffect, useRef } from 'react';\nimport styled from 'styled-components';\nimport { sliderItems } from '../../data';\nimport { mobile } from '../../responsive';\n\nconst Slidercustom = () => {\n  const [slideIndex, setSlideIndex] = useState(0);\n\n  const [current, setCurrent] = useState(0);\n  const length = sliderItems.length;\n  const timeout = useRef(null);\n\n  useEffect(() => {\n    const nextSlide = () => {\n      setCurrent((current) => (current === length - 1 ? 0 : current + 1));\n    };\n\n    timeout.current = setTimeout(nextSlide, 6000);\n    return function () {\n      if (timeout.current) {\n        clearTimeout(timeout.current);\n      }\n    };\n  }, [current, length]);\n\n  const nextSlide = () => {\n    if (timeout.current) {\n      clearTimeout(timeout.current);\n    }\n    setCurrent(current === length - 1 ? 0 : current + 1);\n  };\n\n  const prevSlide = () => {\n    if (timeout.current) {\n      clearTimeout(timeout.current);\n    }\n    setCurrent(current === 0 ? length - 1 : current - 1);\n  };\n\n  if (!Array.isArray(sliderItems) || sliderItems.length <= 0) {\n    return null;\n  }\n\n  return (\n    <Container>\n      <Arrow direction=\"left\" onClick={prevSlide}>\n        <ArrowLeftOutlined />\n      </Arrow>\n      <Wrapper slideIndex={slideIndex}>\n        {sliderItems.map(\n          (item, index) =>\n            current === index && (\n              <Slide bg={item.bg} key={item.id}>\n                <ImgContainer>\n                  <Image src={item.img} />\n                </ImgContainer>\n                <InfoContainer>\n                  <Title>{item.title}</Title>\n                  <Desc>{item.desc}</Desc>\n                  <Button>SHOW NOW</Button>\n                </InfoContainer>\n              </Slide>\n            )\n        )}\n      </Wrapper>\n      <Arrow direction=\"right\" onClick={nextSlide}>\n        <ArrowRightOutlined />\n      </Arrow>\n    </Container>\n  );\n};\n\nexport default Slidercustom;\n\nconst Container = styled.div`\n  width: 100%;\n  height: 100vh;\n  display: flex;\n  position: relative;\n  overflow: hidden;\n  ${mobile({ display: 'none' })}\n  z-index: -1;\n`;\n\nconst Arrow = styled.div`\n  width: 50px;\n  height: 50px;\n  background-color: #fff7f7;\n  border-radius: 50%;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  position: absolute;\n  top: 0;\n  bottom: 0;\n  left: ${(props) => props.direction === 'left' && '10px'};\n  right: ${(props) => props.direction === 'right' && '10px'};\n  margin: auto;\n  cursor: pointer;\n  opacity: 0.5;\n  z-index: 2;\n`;\n\nconst Wrapper = styled.div`\n  height: 100%;\n  display: flex;\n  transition: all 1.5s ease;\n  transform: translateX(${(props) => props.slideIndex * -100}vw);\n`;\n\nconst Slide = styled.div`\n  width: 100vw;\n  height: 100vh;\n  display: flex;\n  align-items: center;\n  background-color: #${(props) => props.bg};\n`;\n\nconst ImgContainer = styled.div`\n  height: 100%;\n  flex: 1;\n`;\n\nconst Image = styled.img`\n  height: 80%;\n`;\n\nconst InfoContainer = styled.div`\n  flex: 1;\n  padding: 50px;\n`;\n\nconst Title = styled.h1`\n  font-size: 70px;\n`;\n\nconst Desc = styled.p`\n  margin: 50px 0px;\n  font-size: 20px;\n  font-weight: 500;\n  letter-spacing: 3px;\n`;\n\nconst Button = styled.button`\n  padding: 10px;\n  font-size: 18px;\n  background-color: transparent;\n  cursor: pointer;\n  border: 1px solid #f4f4f4;\n  border-radius: 5px;\n`;\n","/home/douglas/Pictures/deploy ecommerce to aws/frontend/src/components/layout/Categories.jsx",[],"/home/douglas/Pictures/deploy ecommerce to aws/frontend/src/components/layout/Newsletter.jsx",[],"/home/douglas/Pictures/deploy ecommerce to aws/fron